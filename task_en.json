    "rootPath": {
      "label": "Directory for storing code",
      "placeholder": "Relative path to the workspace. Default is the root directory of the workspace",
      "type": "vuex-input",
      "desc": "If the current job has multiple code pull steps, please set this field to resolve code path conflicts.",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "clientName": {
      "label": "Workspace name",
      "desc": "If the workspace does not exist, it will be created. If left blank, a temporary workspace will be created.",
      "type": "vuex-input",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "depotMode": {
      "label": "",
      "default": "Stream",
      "type": "enum-input",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "list": [
        {
          "label": "Stream",
          "value": "Stream"
        },
        {
          "label": "Manual",
          "value": "Manual"
        }
      ]
    },
    "stream": {
      "label": "Stream path",
      "placeholder": "For example: //JamCode/main",
      "type": "vuex-input",
      "desc": "",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "depotMode",
            "value": "Stream"
          }
        ]
      }
    },
    "view": {
      "label": "Repository mapping",
      "placeholder": "For example: //depot/... //${workspace}/...\n//depot/dir/... //${workspace}/dir2/...",
      "type": "vuex-textarea",
      "desc": "The mapping relationship between P4 depot and workspace",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "depotMode",
            "value": "Manual"
          }
        ]
      }
    },
    "autoCleanup": {
      "text": "Auto cleanup and sync",
      "default": false,
      "desc": "Replace lost/modified files, delete generated files.",
      "type": "atom-checkbox",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": true
    },
    "unshelveId": {
      "label": "Unshelve Id",
      "placeholder": "For example: 526",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "fileRevSpec": {
      "label": "Sync file versions",
      "placeholder": "file.c#4 \n//depot/proj1/...@21",
      "type": "vuex-textarea",
      "desc": "If empty, all files will be synchronized. and perforce's FileSpec and RevSpec are supported, with one version information per line",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "keepGoingOnError": {
      "text": "Continue on Error",
      "default": false,
      "type": "atom-checkbox",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": true
    },
    "forceUpdate": {
      "text": "Force update",
      "default": false,
      "desc": "p4 sync -f",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "noUpdate": {
      "text": "Preview mode",
      "default": false,
      "desc": "p4 sync -n",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "clientBypass": {
      "text": "Skip client update",
      "default": false,
      "desc": "p4 sync -k",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "serverBypass": {
      "text": "Skip server update",
      "default": false,
      "desc": "p4 sync -p",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "quiet": {
      "text": "Quiet mode",
      "default": false,
      "desc": "p4 sync -q",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "safetyCheck": {
      "text": "Safety inspection",
      "default": false,
      "desc": "p4 sync -s",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "max": {
      "label": "Maximum sync quantity",
      "desc": "p4 sync -m",
      "type": "vuex-input",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "netMaxWait": {
      "label": "Maximum sync waiting time(ms)",
      "desc": "Timeout period",
      "type": "vuex-input",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "syncOptions"
    },
    "batch": {
      "label": "Batch quantity",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "parallelSyncOptions"
    },
    "batchSize": {
      "label": "Batch Bytes",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "parallelSyncOptions"
    },
    "minimum": {
      "label": "Minimum number of files to use for concurrent synchronization",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "parallelSyncOptions"
    },
    "minimumSize": {
      "label": "Minimum number of bytes to use for concurrent synchronization",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "parallelSyncOptions"
    },
    "numberOfThreads": {
      "label": "Number of concurrent threads",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "parallelSyncOptions"
    },
    "lineEnd": {
      "label": "LineEnd",
      "placeholder": "LOCAL,UNIX,MAC,WIN,SHARE",
      "desc": "Line ending characters for text files",
      "type": "vuex-input",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "allWrite": {
      "text": "All write",
      "default": false,
      "desc": "If selected, all unopened files will always be writable. By default, the Helix server sets unopened files as read-only.",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "clobber": {
      "text": "Overwrite writable file",
      "default": true,
      "desc": "If selected, p4 sync will overwrite writable but unopened workspace files. (By default, if unopened files are writable, the Helix server will not overwrite them.)",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "compress": {
      "text": "Compressed transmission",
      "default": false,
      "desc": "If selected, when communicating between the computer and the Helix server, data compression is applied",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "locked": {
      "text": "Lock",
      "default": false,
      "desc": "If selected, other users can use, edit, or delete client workspace specifications. Helix server administrators can use the -f (force) option to override locks.",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "modtime": {
      "text": "File modified time",
      "default": false,
      "desc": "For files without the \"+m\" (modtime) file type modifier, if this option is selected, the modification date of the newly synchronized file will be the timestamp when the file was committed to the repository. If not selected, the modification date will be the date and time of synchronization.",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    },
    "rmdir": {
      "text": "Delete dir",
      "default": false,
      "desc": "If selected, When using 'p4 sync' to delete all files in the workspace, empty directories will also be deleted.",
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "groupName": "fileOptions"
    }
  },
  "inputGroups": [
    {
      "name": "fileOptions",
      "label": "File Options",
      "isExpanded": false
    },
    {
      "name": "syncOptions",
      "label": "Sync Options",
      "isExpanded": false
    },
    {
      "name": "parallelSyncOptions",
      "label": "Concurrent Synchronization Options (Requires p4 server support --parallel flag, older server versions may not support)",
      "isExpanded": false
    }
  ],
  "output": {
    "BK_CI_P4_REPO_HEAD_CHANGE_ID": {
      "type": "string",
      "description": "When pulling the code, the last change id of this build"
    },
    "BK_CI_P4_DEPOT_HEAD_CHANGE_COMMENT": {
      "type": "string",
      "description": "When pulling the code, the last change comment of this build"
    },
    "BK_CI_P4_DEPOT_HEAD_CHANGE_CLIENT_ID": {
      "type": "string",
      "description": "When pulling the code, the last change comment of this build"
    },
    "BK_CI_P4_DEPOT_HEAD_CHANGE_USER": {
      "type": "string",
      "description": "The user who built the last change this time"
    },
    "BK_CI_P4_DEPOT_LAST_CHANGE_ID": {
      "type": "string",
      "description": "When pulling the code, the last change id of the last build"
    },
    "BK_CI_P4_DEPOT_WORKSPACE_PATH": {
      "type": "string",
      "description": "The local storage path of the current code repository"
    },
    "BK_CI_P4_DEPOT_PORT": {
      "type": "string",
      "description": "The URL of the p4 server"
    },
    "BK_CI_P4_DEPOT_STREAM": {
      "type": "string",
      "description": "Stream path"
    },
    "BK_CI_P4_DEPOT_P4_CHARSET": {
      "type": "string",
      "description": "Pull the charset used"
    }
  }
}