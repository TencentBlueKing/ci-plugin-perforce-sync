{
  "atomCode": "PerforceSync",
  "defaultLocaleLanguage": "en_US",
  "execution": {
    "packagePath": "PerforceSync-jar-with-dependencies.jar",
    "language": "java",
    "minimumVersion": "1.8",
    "demands": [],
    "target": "$bk_java_path -Dfile.encoding=utf8 -jar PerforceSync-jar-with-dependencies.jar"
  },
  "releaseInfo": {
    "projectId": "demo",
    "name": "Pull P4",
    "language": "java",
    "logoUrl": "${{indexFile(\"logo.png\")}}",
    "category": "TASK",
    "classifyCode": "scm",
    "jobType": "AGENT",
    "os": [
      "LINUX",
      "MACOS",
      "WINDOWS"
    ],
    "labelCodes": [],
    "summary": "Full-featured implementation p4 sync",
    "description": "${{indexFile(\"README.md\")}}",
    "configInfo": {
      "frontendType": "NORMAL"
    },
    "versionInfo": {
      "publisher": "BlueKing",
      "releaseType": "NEW",
      "version": "1.0.0"
    }
  },
  "input": {
    "repositoryType": {
      "label": "Repository",
      "default": "URL",
      "desc": "Based on input method, Filed value is ID or Alias Name or URL",
      "type": "enum-input",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "list": [
        {
          "label": "Select Repository",
          "value": "ID"
        },
        {
          "label": "Enter by Repository Alias Name",
          "value": "NAME"
        },
        {
          "label": "Enter by Repository URL",
          "value": "URL"
        }
      ]
    },
    "repositoryHashId": {
      "label": "",
      "default": "",
      "placeholder": "Please Select Repository ID",
      "type": "selector",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "optionsConf": {
        "searchable": true,
        "multiple": false,
        "url": "/repository/api/user/repositories/{projectId}/hasPermissionList?permission=USE&page=1&pageSize=5000&repositoryType=CODE_P4",
        "paramId": "repositoryHashId",
        "paramName": "aliasName",
        "itemTargetUrl": "/codelib/{projectId}/",
        "itemText": "Repository Association",
        "hasAddItem": true
      },
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "repositoryType",
            "value": "ID"
          }
        ]
      }
    },
    "repositoryName": {
      "label": "",
      "default": "",
      "placeholder": "Please enter repository alias name",
      "type": "vuex-input",
      "desc": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "repositoryType",
            "value": "NAME"
          }
        ]
      }
    },
    "p4port": {
      "label": "",
      "placeholder": "Please enter p4 server url, for example: localhost:1666",
      "type": "vuex-input",
      "desc": "",
      "default": "",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "repositoryType",
            "value": "URL"
          }
        ]
      }
    },
    "ticketId": {
      "label": "Repository Credential",
      "placeholder": "Please select the corresponding certificate",
      "type": "selector",
      "desc": " Credential support two types of username+password and ticket, Use the '[USERNAME + PASSWORD]' type of credential, and the password field should be filled with either the p4 password or ticket",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "optionsConf": {
        "searchable": true,
        "multiple": false,
        "url": "/ticket/api/user/credentials/{projectId}/hasPermissionList?permission=USE&credentialTypes=USERNAME_PASSWORD&page=1&pageSize=10000",
        "paramId": "credentialId",
        "paramName": "credentialId",
        "itemTargetUrl": "/ticket/{projectId}/createCredential/USERNAME_PASSWORD",
        "itemText": "Add New Credential",
        "hasAddItem": true
      },
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "repositoryType",
            "value": "URL"
          }
        ]
      }
    },
    "proxy_flag": {
      "text": "Enable proxy",
      "default": false,
      "desc": "",
      "type": "atom-checkbox",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": true
    },
    "httpProxy": {
      "label": "HTTP proxy address",
      "placeholder": "",
      "type": "vuex-input",
      "desc": "Please enter the http proxy address",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "proxy_flag",
            "value": true
          }
        ]
      }
    },
    "charsetName": {
